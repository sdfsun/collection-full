        career,
      </if>
      <if test="familyPhone != null" >
        family_phone,
      </if>
      <if test="mobile != null" >
        mobile,
      </if>
      <if test="relation1 != null" >
        relation_1,
      </if>
      <if test="personnel1 != null" >
        personnel_1,
      </if>
      <if test="caseNum1 != null" >
