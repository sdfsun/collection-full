      </if>
      <if test="remark != null" >
        #{remark},
      </if>
    </trim>
  </insert>
 
  <update id="update">
    update census_register
    <set >
      <if test="caseApplyId != null" >
        case_apply_id = #{caseApplyId},
      </if>
      <if test="crAddress != null" >
        cr_address = #{crAddress},
      </if>
      <if test="address != null" >
        address = #{address},
      </if>
      <if test="married != null" >
        married = #{married},
      </if>
      <if test="mate != null" >
        mate = #{mate},
      </if>
      <if test="culture != null" >
        culture = #{culture},
      </if>
      <if test="career != null" >
        career = #{career},
      </if>
