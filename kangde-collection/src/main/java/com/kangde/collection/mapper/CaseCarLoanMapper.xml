<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.kangde.collection.mapper.CaseCarLoanMapper">

	<insert id="save">
		INSERT INTO `case_car_loan`
(id,case_id,dealer,price,lice_no,brand,number,vin_no,engine_no)
